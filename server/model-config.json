{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "../node_modules/loopback-disable-all-remote-methods-mixin",
      "./mixins"
    ]
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "user": {
    "dataSource": "mydb",
    "public": true
  },
  "item": {
    "dataSource": "mydb",
    "public": true
  },
  "cart": {
    "dataSource": "mydb",
    "public": true
  },
  "cart-item": {
    "dataSource": "mydb",
    "public": true
  }
}
